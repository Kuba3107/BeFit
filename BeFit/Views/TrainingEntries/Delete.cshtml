@model BeFit.Models.TrainingEntry

@{
    ViewData["Title"] = "Delete";
}

<h1>Czy na pewno chcesz usunąć ten wpis?</h1>
<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TrainingSession)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TrainingSession.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ExerciseType)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ExerciseType.Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Load)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Load)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Sets)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Sets)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Reps)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reps)
        </dd>
    </dl>
    
    <form class="btn-group" asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input class="btn btn-danger" type="submit" value="Usuń" class="btn btn-danger" />
        <a class="btn btn-primary" asp-action="Index">Powrót</a>
    </form>
</div>
